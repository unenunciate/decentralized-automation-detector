{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
		"module": "CommonJS",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "./dist",
    "esModuleInterop": true, 
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "src",
    "paths": {
			"@/*": [ "*" ]
		}
  }
}